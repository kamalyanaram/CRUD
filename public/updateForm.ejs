<body>


    <form action="/updateFilm" method="post" class="d-flex justify-content-center">
        <input id="usName" type="text" required name="name" placeholder="Please type the film name" class="me-2" value="<%= obj.name %>"><br><br>
        <input id="usData" type="date" name="data" placeholder="Please type the film data" class="me-2" value="<%= obj.data %>"><br><br>
        <input type="hidden" name="id" value="<%= obj._id %>">
        <input type="submit" >
    </form>

